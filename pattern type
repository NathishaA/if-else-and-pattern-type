pramid
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int row=5;
    
    for(int i=1;i<=row;i++)
    {
        for(int k=1;k<=row-i;k++)
        {
            cout<<" ";
        }
        
        for(int j=1;j<=2*i-1;j++)
        {
          if(j==1 || j==2*i-1 || i==row)
          {
            cout<<"*";
          }
          else{
              cout<<" ";
          }
        }
        cout<<"\n";
    }
}

hallow pramid

#include<bits/stdc++.h>
using namespace std;

int main()
{
    int row=5;
    
    for(int i=1;i<=row;i++)
    {
        for(int k=1;k<=row-i;k++)
        {
            cout<<" ";
        }
        
        for(int j=1;j<=2*i-1;j++)
        {
          if(j==1 || j==2*i-1 )
          {
            cout<<"*";
          }
          else{
              cout<<" ";
          }
        }
        cout<<"\n";
    }
    
    for(int i=row-1;i>=1;i--)
    {
        for(int k=1;k<=row-i;k++)
        {
            cout<<" ";
        }
        
        for(int j=1;j<=2*i-1;j++)
        {
          if(j==1 || j==2*i-1)
          {
            cout<<"*";
          }
          else
          {
              cout<<" ";
          }
        }
        cout<<"\n";
    }
}
