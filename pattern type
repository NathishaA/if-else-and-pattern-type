pattern loop
square 
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int row=4;
    int col=3;
    
    for(int i=1;i<=row;i++)
    {
        for(int j=1;j<=col;j++)
        {
            cout<<" * ";
        }
        cout<<"\n";
    }
}

hallow 
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int row=4;
    int col=4;
    
    for(int i=1;i<=row;i++)
    {
        for(int j=1;j<=col;j++)
        {
            if(i==1 || i==row || j==1|| j==col)
            {
                cout<<"*";
            }
            
            else
            {
                cout<<" ";
            }
        }
        
        cout<<"\n";
    }
}

