num pattern print
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int count=1;
    int row=4;
    int col=3;
    
    for(int i=1;i<=row;i++)
    {
        for(int j=1;j<=i;j++)
        {
            cout<<count<<" ";
            count++;
        }
        
        cout<<"\n";
    }
}

