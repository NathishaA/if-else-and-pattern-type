}

#include<bits/stdc++.h>
using namespace std;

int main()
{
    int c1=1;
    int c2=2;
    int row=2;
    int col=2;
    
    for(int i=1;i<=row;i++)
    {
        for(int j=1;j<=i;j++)
        {
        if(i%2==0)
        {
            cout<<c2<<" ";
            c2+=2;
        }
        
        else
        {
            cout<<c1;
            c1+=2;
        }
        }
    
    cout<<"\n";
    }
}

mirror of right anglle..... cout la * pakaam place vitta triangle varum.
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int row=4;
    
    for(int i=1;i<=row;i++)
    {
        for(int k=1;k<=row-i;k++)
        {
            cout<<" ";
        }
        
        for(int j=1;j<=i;j++)
        {
            cout<<"*";
        }
        
        cout<<"\n";
    }
}

join the 2 triangle
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int row=4;
    
    for(int i=1;i<=row;i++)
    {
        for(int s=1;s<=i;s++)
        {
            cout<<"*";
        }
        
        for(int k=1;k<=2*(row-i);k++)
        {
            cout<<" ";
        }
        
        
        for(int j=1;j<=i;j++)
        {
            cout<<"*";
        }
        
        cout<<"\n";
    }
}

buterfly 
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int row=4;
    
    for(int i=1;i<=row;i++)
    {
        for(int h=1;h<=i;h++)
        {
            cout<<"*";
        }
        
        for(int s=1;s<=2*(row-i);s++)
        {
            cout<<" ";
        }
        
        
        for(int j=1;j<=i;j++)
        {
            cout<<"*";
        }
        
        cout<<"\n";
    }
    for(int i=row-1;i>=1;i--)
    {
        for(int s=1;s<=i;s++)
        {
            cout<<"*";
        }
        
        for(int k=1;k<=2*(row-i);k++)
        {
            cout<<" ";
        }
        
        
        for(int j=1;j<=i;j++)
        {
            cout<<"*";
        }
        
        cout<<"\n";
    }
}

