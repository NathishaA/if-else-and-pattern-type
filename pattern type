#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    
    if((n%400==0) && ((n%4==0) && (n!=100)))
    {
        cout<<"Its a leap year";
    }
    
    else
    {
        cout<<"Not a leap year";
    }
}

#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin>>n;
    int row=3;
    int col=4;

    if((n>=1 && n<=col) || (n%col==1 && n%4==0))
    {
        cout<<"Mango Tree";
    }
    
    else 
    {
        cout<<"Apple Tree";
    }
}

